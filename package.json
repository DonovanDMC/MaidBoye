    "@uwu-codes/utils": "^1.3.4",
    "blapi": "^3.1.2",
    "cheweyapi": "^2.0.4",
    "chunk": "^0.0.3",
    "debug": "^4.3.5",
    "discord-arts": "^0.6.1",
    "discord-command-parser": "^1.5.3",
    "e621": "^2.2.6",
