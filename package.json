{
  "name": "maidboye",
  "version": "2.0.0",
  "description": "A cute little maid bot for keeping your server nice and tidy.",
  "buildDate": null,
  "main": "index.js",
  "scripts": {
    "build": "tsc && node --no-warnings --no-deprecation --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only scripts/build-date.ts && echo Build Finished",
    "create-migration": "node -e \"require('fs').writeFileSync('./src/db/migrations/' + Date.now() + '_' + (process.argv[1] || 'name') + '.sql', '')\"",
    "postinstall": "patch-package",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "down": "sudo docker compose down --volumes --remove-orphans",
    "up:d": "sudo docker compose --profile all up --build --force-recreate -d",
    "up:db": "sudo docker compose --profile db up --build --force-recreate -d",
    "up:dev": "sudo docker compose --profile all up",
    "dev": "npm run down && npm run up:dev",
    "update": "npm run down && git pull && git submodule update --init --recursive --remote && npm run up:d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DonovanDMC/MaidBoye.git"
  },
  "keywords": [
    "maid",
    "bot",
    "maid-boye",
    "discord-bot",
    "eris"
  ],
  "author": "Donovan Daniels <hewwo@yiff.rocks> (https://furry.cool)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DonovanDMC/MaidBoye/issues"
  },
  "homepage": "https://github.com/DonovanDMC/MaidBoye#readme",
  "devDependencies": {
    "@swc/core": "^1.3.21",
    "@swc/helpers": "^0.4.14",
    "@types/chunk": "^0.0.0",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/fuzzy-search": "^2.1.2",
    "@types/markdown-it": "^12.2.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.10",
    "@types/pg": "^8.6.5",
    "@uwu-codes/eslint-config": "^1.1.6",
    "@uwu-codes/tsconfig": "^1.0.2",
    "@uwu-codes/types": "^1.0.6",
    "eslint-plugin-import": "^2.26.0",
    "sagiri": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@oceanicjs/builders": "^1.1.7",
    "@uwu-codes/utils": "^1.1.7",
    "cheweyapi": "^1.0.3",
    "chunk": "^0.0.3",
    "dashargs": "^4.1.5",
    "debug": "^4.3.4",
    "discord-command-parser": "^1.5.3",
    "e621": "^2.2.6",
    "eslint-plugin-unicorn": "^45.0.1",
    "express": "^4.18.2",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "file-type": "^18.0.0",
    "fuzzy-search": "^3.2.1",
    "imgen": "^1.0.1",
    "ioredis": "^5.2.4",
    "markdown-it": "^13.0.1",
    "morgan": "^1.10.0",
    "oceanic.js": "^1.3.3-dev.47f1718",
    "patch-package": "^6.5.0",
    "pg": "^8.8.0",
    "short-uuid": "^4.2.2",
    "tsafe": "^1.4.1",
    "tslog": "^3.3.4",
    "yiffy": "^2.3.0"
  },
  "type": "module",
  "engines": {
    "node": ">=19.x"
  },
  "engineStrict": true
}
